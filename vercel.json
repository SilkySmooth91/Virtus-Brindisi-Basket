{
  "buildCommand": "cd 'virtus-brindisi-basket' && npm run build",
  "outputDirectory": "virtus-brindisi-basket/dist",
  "installCommand": "cd 'virtus-brindisi-basket' && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/news/(\\d+)",
      "destination": "/api/news/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}